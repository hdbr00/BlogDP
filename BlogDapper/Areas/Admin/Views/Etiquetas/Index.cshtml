@model IEnumerable<BlogDapper.Models.Etiqueta>
@{
    ViewData["Title"] = "Lista de Etiquetas";
}
<div class="row">
    <div class="col-9">
        <h1>Administrador de etiquetas</h1>
    </div>
    <div class="col-3">
        <a asp-action="Crear" class="btn btn-primary float-end"><i class="bi bi-plus-square"></i> Agregar Etiquetas</a>
    </div>
</div>
<div class="card bg-light p-3">
    <table class="table" id="tblEtiquetas">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Fecha de Cración</th>
                <th>Acciones</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/etiquetas.js"></script>

}